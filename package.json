{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"name": "",
	"version": "1.0.0",
	"description": "",
	"devDependencies": {
		"@wordpress/eslint-plugin": "^17.12.0",
		"@wordpress/prettier-config": "^3.12.0",
		"autoprefixer": "^10.4.19",
		"cssnano": "^6.1.2",
		"postcss": "^8.4.38",
		"postcss-cli": "^11.0.0",
		"sass": "^1.75.0"
	},
	"scripts": {
		"editor-js": "wp-scripts start src/js/editor/index.js --output-path=assets/js/editor --output-filename=editor.js",
		"watch": "sass --watch --update --style=expanded src/scss:assets/css",
		"sass": "sass --style=expanded src/scss:assets/css",
		"build": "sass --no-source-map --style=compressed src/scss:assets/css && postcss --no-map assets/css/**/*.css --replace --use autoprefixer cssnano"
	},
	"browserslist": [
		"> 1%",
		"not IE 11"
	],
	"prettier": "@wordpress/prettier-config"
}
